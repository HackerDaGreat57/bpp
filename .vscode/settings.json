  "saveAndRun": {
    "commands": [
      {
        "match": ".*",
        "cmd": "%BPP_ROOT%/scripts/build.windows.bat",
        "useShortcut": false,
        "silent": true
      }
    ]
  },
  "editor.cursorStyle": "block",
  "files.autoSave": "afterDelay",
  "editor.fontFamily": "Cascadia Code, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.smoothScrolling": true,
  "editor.suggest.preview": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 1,
  "editor.minimap.showSlider": "mouseover",
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "window.titleSeparator": " | ",
  "bookmarks.label.suggestion": "useWhenSelected",
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "bookmarks.keepBookmarksOnLineDelete": true,
  "gitlens.outputLevel": "debug",
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.codeLens.authors.command": "gitlens.toggleFileHeatmap",
  "gitlens.statusBar.reduceFlicker": false,
